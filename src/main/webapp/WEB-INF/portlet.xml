<?xml version="1.0"?>

<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" version="2.0">
	<portlet>
		<portlet-name>map-event-consumer</portlet-name>
		<display-name>map-event-consumer</display-name>
		<portlet-class>im.ligas.mapjs.portlet.wc.WebContentDetailPortlet</portlet-class>
		<init-param>
			<name>view-template</name>
			<value>/html/wc/view.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
		</supports>
		<portlet-info>
			<title>map-event-consumer</title>
			<short-title>map-event-consumer</short-title>
			<keywords>map-event-consumer</keywords>
		</portlet-info>
	</portlet>
	<portlet>
		<portlet-name>map-to-table</portlet-name>
		<display-name>map-to-table</display-name>
		<portlet-class>im.ligas.mapjs.portlet.table.TableDetailPortlet</portlet-class>
		<init-param>
			<name>view-template</name>
			<value>/html/table/view.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
		</supports>
		<portlet-info>
			<title>map-to-table</title>
			<short-title>map-to-table</short-title>
			<keywords>map-to-table</keywords>
		</portlet-info>
	</portlet>
	<portlet>
		<portlet-name>map-to-line-chart</portlet-name>
		<display-name>map-to-line-chart</display-name>
		<portlet-class>im.ligas.mapjs.portlet.graph.LineChartPortlet</portlet-class>
		<init-param>
			<name>view-template</name>
			<value>/html/graph/view-line.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
		</supports>
		<portlet-info>
			<title>map-to-line-chart</title>
			<short-title>map-to-line-chart</short-title>
			<keywords>map-to-line-chart</keywords>
		</portlet-info>
	</portlet>
	<portlet>
		<portlet-name>map-to-line-pie</portlet-name>
		<display-name>map-to-line-pie</display-name>
		<portlet-class>im.ligas.mapjs.portlet.graph.PieChartPortlet</portlet-class>
		<init-param>
			<name>view-template</name>
			<value>/html/graph/view-pie.jsp</value>
		</init-param>
		<init-param>
			<name>edit-template</name>
			<value>/html/graph/edit.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
			<portlet-mode>edit</portlet-mode>
		</supports>
		<portlet-info>
			<title>map-to-line-pie</title>
			<short-title>map-to-line-pie</short-title>
			<keywords>map-to-line-pie</keywords>
		</portlet-info>
	</portlet>
</portlet-app>
